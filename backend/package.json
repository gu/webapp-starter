{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:tsoa": "tsoa spec-and-routes --basePath /api",
    "build:drizzle": "drizzle-kit generate:pg --config ./src/db/drizzle.config.ts",
    "build:swc": "swc src build -d dist --copy-files",
    "build:dev": "npm run build:clean && npm run build:tsoa && npm run build:swc",
    "build": "npm run build:clean && npm run build:tsoa && npm run build:drizzle && npm run build:swc",
    "start:dev": "nodemon --watch src --ext ts --exec 'npm run build:dev && node dist/src/index.js'",
    "start": "npm run migrate:up && node dist/src/index.js",
    "migrate:up": "node dist/src/db/migrate.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "api-schemas": "workspace:*",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.16",
    "@tsoa/runtime": "^6.2.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.9",
    "envalid": "^8.0.0",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.11.5",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/swagger-ui-express": "^4.1.6",
    "drizzle-kit": "^0.20.17",
    "nodemon": "^3.1.0"
  }
}
